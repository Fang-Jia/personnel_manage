<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.xf.entity.Card" table="t_card">
        <id name="cid" column="c_id">
            <generator class="native"/>
        </id>
        <property name="name" column="d_name"/>
        <property name="deptName" column="dept_name"/>
        <property name="checkTime" column="check_time"/>
        <property name="cardTime" column="card_time"/>
        <property name="reason" column="reason"/>
        <property name="checkName" column="check_name"/>
        <property name="deptLeader" column="dept_leader"/>
        <property name="status" column="status"/>

    </class>
</hibernate-mapping>